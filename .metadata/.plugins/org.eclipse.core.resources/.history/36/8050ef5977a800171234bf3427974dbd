package partA;

public class UniqueRegisterNumber {
	
	// data fields
	static int uniqueRegisterNumber = 0;
	int registerNumber = 0;
	
	// constructor
	public UniqueRegisterNumber() {
		uniqueRegisterNumber++;
		registerNumber = uniqueRegisterNumber;
	}
	
	// getUniqueRegister
	public int getUniqueRegisterNumber() {
		return registerNumber;
	}

}